@{
    ViewBag.Title = ViewBag.EventKey + " Gossip";
}

<h2>@ViewBag.Title</h2>

@section Scripts {
    <script>
        (function() {
            var eventKey = '@ViewBag.EventKey';
            var hub = $.connection.eventHub;
            $.connection.hub.start().done(function() {
                hub.server.join(eventKey, 'Tats');
                console.log('Connected');
            });
        })();
    </script>
}
